.index {
  background: #f4f4f4;
}

.index .search {
  width: 750rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20rpx 20rpx 0 20rpx;
  background: #fff;
}

.index .search text {
  width: 128rpx;
  font-size: 26rpx;
  line-height: 56rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  /* margin-right: 30rpx; */
}

.index .search .inp {
  width: 580rpx;
  height: 56rpx;
  display: flex;
  align-items: center;
  margin: 0 40rpx 0 15rpx;
  background: #ededed;
  border-radius: 10rpx;
}

.index .search .inp image {
  height: 28rpx;
  width: 28rpx;
  padding: 0 10rpx;
}

/* 轮播图 */

.index .swiper .swiperimg {
  width: 100%;
}

/* 商品分类列表 */

.index .goodslist {
  width: 750rpx;
  height: 144rpx;
  display: flex;
  justify-content: space-between;
  background: #fff;
}

.index .goodslist .goodbox navigator {
  width: 150rpx;
  height: 144rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.index .goodslist .goodbox image {
  height: 58rpx;
  width: 58rpx;
}

.index .goodslist .goodbox text {
  font-size: 28rpx;
  color: #333;
}

/* 品牌制造商楼层 */

.index .brand {
  height: 630rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
}

.index .brand .title {
  height: 120rpx;
  width: 100%;
  line-height: 120rpx;
  color: #333;
  text-align: center;
  border-top: 20rpx solid #f4f4f4;
}

.index .brand .main {
  height: 510rpx;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.index .brand .main .item {
  height: 254rpx;
  width: 359rpx;
  display: inline-flex;
  overflow: hidden;
  border: 7rpx solid #fff;
  position: relative;
}

.index .brand .main .item .backgroundImg image {
  width: 380rpx;
  height: 254rpx;
}

.index .brand .main .item text {
  position: absolute;
  top: 10rpx;
  left: 0;
  color: #333;
}

.index .brand .main .item text.smallfont {
  top: 50rpx;
  font-size: 24rpx;
}

/* 新品首发 */

.index .newgoods {
  margin-top: 60rpx;
  width: 100%;
  height: 260rpx;
}

.index .newgoods .bgbox {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 260rpx;
  background: url(data:image/png;base64,) no-repeat;
}

.index .newgoods .bgbox text {
  color: #8c9bae;
  font-size: 32rpx;
  line-height: 50rpx;
}

.index .newgoods .bgbox text.lookall {
  display: block;
  text-align: center;
  width: 180rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 27rpx auto 0 auto;
  font-size: 22rpx;
  background: #d8e4f0;
}

.index .newgoods.two {
  margin-top: 10rpx;
  width: 100%;
  height: 260rpx;
}

.index .newgoods.two .bgbox {
  background: url(data:image/png;base64,) no-repeat;
}

.index .newgoods.two text {
  color: #b1a279;
  font-size: 32rpx;
}

.index .newgoods.two .bgbox text.lookall {
  background: #f4e9cb;
}

/* 专题精选 */

.index .topicList {
  height: 570rpx;
  width: 100%;
  padding-bottom: 20rpx;
  display: flex;
  flex-direction: column;
  /* background: green; */
  font-size: 28rpx;
  color: #333;
  margin-top: 10rpx;
}

.index .topicList .title {
  line-height: 100rpx;
  text-align: center;
  background: #fff; 
  vertical-align: center
}

.index .topicList .title .titleMain {
  padding-right: 10rpx;
}

.index .topicList .title image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}

.index .topicList scroll-view {
  height: 454rpx;
  white-space: nowrap;
  background: #fff;
}

.index .topicList scroll-view .goodsitem {
  width: 574rpx;
  padding: 0rpx 15rpx 15rpx 15rpx;
  display: inline-flex;
  flex-direction: column;
}

.index .topicList scroll-view .goodsitem image {
  height: 324rpx;
  width: 574rpx;
}

.index .topicList scroll-view .goodsitem .title {
  display: flex;
  justify-content: space-between;
  height: 40rpx;
  line-height: 40rpx;
}

.index .topicList scroll-view .goodsitem .title .name {
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 8rpx;
}

.index .topicList scroll-view .goodsitem .title .money {
  margin-top: 8rpx;
  color: #9c3232;
  font-size: 24rpx;
}

.index .topicList scroll-view .goodsitem .desc {
  width: 90%;
  color: #8a8a8a;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-top: 10rpx;
line-height: 40rpx;
}
