<!--pages/editAddress/main.wxml-->
<view class="main">
    <view class="address" wx:for="{{addrlist}}"
    wx:key="{{index}}" catchtap="selectAddress" wx:for-item="address">
    <view class="name">
      <text>{{address.name}}</text>
      <text>{{address.mobile}}</text>
    </view>
    <view class="addr">
      <text class="{{index===0?'default':'normal'}}">默认</text>
      <text>{{address.address}}{{address.address_detail}}</text>
    </view>
    <image data-index="{{index}}" catchtap="reEditThisAddress" src="data:image/png;base64,">
    </image>
  </view>


  <!-- 新建地址栏 -->
   <view class="newAddrBox">
      <view class="new">
         +新建地址
      </view>
      <view class="oneExport">
        一键导入微信地址
      </view>
   </view>

</view>