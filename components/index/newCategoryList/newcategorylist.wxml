<!--components/index/newCategoryList/newcategorylist.wxml-->
<view class="main">
   <view class="title">{{name}}好物</view>
   <view class="content">
       <view wx:for="{{goodsList}}" wx:key="{{index}}" class="item">
          <image src="{{item.list_pic_url}}"></image>
          <text class="name">{{item.name}}</text>
          <text class="money">￥{{item.retail_price}}</text>
       </view>
       <view class="moreGoods">
            <text>{{name}}好物</text>
            <image src="data:image/png;base64,"></image>
       </view>
   </view>    
</view>
